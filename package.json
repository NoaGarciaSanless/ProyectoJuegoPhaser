    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "keywords": [
        "phaser",
        "phaser3",
        "typescript",
        "vue",
        "vite"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "preview": "vite preview --port 8080 --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs"
    },
    "dependencies": {
        "bootstrap": "^5.3.6",
        "cloudinary": "^2.6.0",
        "firebase": "^11.6.1",
        "phaser": "^3.87.0",
        "phaser3-rex-plugins": "^1.80.13",
        "pixi.js": "^8.9.2",
        "vue": "^3.4.27"
    },
    "devDependencies": {
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "^20.11.20",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/tsconfig": "^0.5.1",
        "terser": "^5.31.0",
        "typescript": "~5.3.3",
        "vite": "^5.2.11",
        "vue-tsc": "^2.0.6"
    }
